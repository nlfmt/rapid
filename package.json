{
  "name": "@nlfmt/rapid",
  "version": "1.1.0",
  "description": "Rapidly create typesafe routes for your express application",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --minify",
    "release": "pnpm run build && changeset publish",
    "lint": "tsc",
    "dev": "pnpm link -g && pnpm link -g @nlfmt/rapid && tsup src/index.ts --watch --dts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nlfmt/rapid.git"
  },
  "bugs": {
    "url": "https://github.com/nlfmt/rapid/issues"
  },
  "homepage": "https://github.com/nlfmt/rapid#readme",
  "keywords": [
    "express",
    "router",
    "routing",
    "typescript",
    "typesafety"
  ],
  "author": "nlfmt <nlfmt@gmx.de>",
  "license": "MIT",
  "dependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@typeschema/main": "^0.14.0",
    "@typeschema/zod": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.19.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.16.10",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  }
}
